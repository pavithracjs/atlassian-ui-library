{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": [
      "es5",
      "scripthost",
      "es2015.core",
      "es2015.collection",
      "es2015.symbol",
      "es2015.iterable",
      "es2015.promise",
      "es2016",
      "es2017"
    ],
    "types": ["node"],
    "typeRoots": ["../../../typings", "./typings", "./node_modules/@types"]
  },
  "include": [
    "./src/**/*.ts",
    "./typings/yarnpkg-lockfile.d.ts",
    "./typings/projector-spawn.d.ts"

  ],

}
