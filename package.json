  "scripts": {
    "build": "                               yarn build:create:folder:entry-points && run-p build:babel build:flowtypes && run-p build:typescript build:adf-schema:json-schema --print-label --continue-on-error && yarn build:exception-packages && yarn copy:version && yarn check:dists",
    "build:exception-packages": "            yarn build:media-editor:copy-binaries && yarn build:css-reset && yarn build:reduced-ui-pack && yarn build:email-renderer && yarn build:util-data-test",
    "build:babel": "                         run-p build:babel:cjs build:babel:esm",
    "build:babel:cjs": "                     NODE_ENV=production BABEL_ENV=production:cjs bolt workspaces exec --parallel --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js babel)\" -- babel src -d dist/cjs --root-mode upward",
    "build:babel:esm": "                     NODE_ENV=production BABEL_ENV=production:esm bolt workspaces exec --parallel --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js babel)\" -- babel src -d dist/esm --root-mode upward",
    "build:typescript": "                    yarn build:typescript:cjs && yarn build:typescript:esm && yarn build:typescript:cli",
    "build:typescript:cli": "                NODE_ENV=production bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js typescriptcli)\" -- bash -c 'tsc --project ./build/cli || true'",
    "build:typescript:cjs": "                NODE_ENV=production bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js typescript)\" -- bash -c 'tsc --project ./build/tsconfig.json --outDir ./dist/cjs --module commonjs || true'",
    "build:typescript:esm": "                NODE_ENV=production bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js typescript)\" -- bash -c 'tsc --project ./build/tsconfig.json --outDir ./dist/esm --module esnext || true'",
    "build:create:folder:entry-points": "    node build/ci-scripts/create.entry.points.directories.js",
    "build:flowtypes": "                     run-p build:flowtypes:cjs build:flowtypes:esm ",
    "build:flowtypes:cjs": "                 bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js babel flow)\" -- flow-copy-source -v -i '**/__tests__/**' src dist/cjs",
    "build:flowtypes:esm": "                 bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js babel flow)\" -- flow-copy-source -v -i '**/__tests__/**' src dist/esm",
    "build:adf-schema:json-schema": "        cd packages/editor/adf-schema && yarn run build:json-schema && echo 'Done.'",
    "build:editor-mobile-bridge": "          cd packages/editor/editor-mobile-bridge && yarn build",
    "build:media-editor:copy-binaries": "    cd packages/media/media-editor && cp -r src/engine/core/binaries dist/esm/engine/core && cp -r src/engine/core/binaries dist/cjs/engine/core && cd - && yarn check:media-editor",
    "build:navigation-next": "               cd packages/core/navigation-next && yarn build",
    "build:bundle-analysis": "               node ./build/ci-scripts/run.bundle.analysis",
    "build:css-reset": "                     bolt w @atlaskit/css-reset run build",
    "build:reduced-ui-pack": "               BABEL_ENV=production:esm bolt w @atlaskit/reduced-ui-pack run build",
    "build:email-renderer": "                bolt w @atlaskit/email-renderer run build",
    "build:adf-schema:checkuptodate": "      cd packages/editor/adf-schema && yarn run generate:json-schema && git diff --exit-code",
    "build:multi-entry-point-tsconfig": "    node ./build/multi-entry-tools/typescript-paths-tsconfig-builder.js > tsconfig.entry-points.json",
    "build:util-data-test": "                cd packages/elements/util-data-test && cp -r src/json-data dist/esm/json-data && cp -r src/json-data dist/cjs/json-data",
    "check:typescript-dists": "              NODE_ENV=production bolt workspaces exec --only-fs \"$(node ./build/ci-scripts/get.glob.packages.for.tools.js typescript)\" -- node ../../../build/utils/sanity-check-file-structure-dist.js",
    "check:dists": "                         node ./build/utils/sanity-check-file-structure-dist.js && yarn check:typescript-dists",
    "check:media-editor": "                  cd packages/media/media-editor/dist/esm/engine/core/binaries && cd - && cd packages/media/media-editor/dist/cjs/engine/core/binaries",
    "changeset": "                           changeset",
    "copy:version": "                        bolt workspaces exec --ignore-fs '{packages/editor/updater-cli,packages/monorepo-tooling/code-insights,packages/monorepo-tooling/dependency-version-analytics,packages/monorepo-tooling/branch-deploy-product-integrator}' --only-fs 'packages/*/*' -- mkdir -p dist/{cjs,esm} && bolt workspaces exec  --only-fs 'packages/*/*' -- copy-pkg package.json tmp/package.json --only name,version,sideEffects && bolt workspaces exec --ignore-fs '{packages/editor/updater-cli,packages/monorepo-tooling/code-insights,packages/monorepo-tooling/dependency-version-analytics,packages/monorepo-tooling/branch-deploy-product-integrator}' --only-fs 'packages/*/*' -- cp ./tmp/package.json ./dist/cjs/version.json && bolt workspaces exec --ignore-fs '{packages/editor/updater-cli,packages/monorepo-tooling/code-insights,packages/monorepo-tooling/dependency-version-analytics,packages/monorepo-tooling/branch-deploy-product-integrator}' --only-fs 'packages/*/*' -- cp ./tmp/package.json ./dist/esm/version.json && bolt workspaces exec --only-fs '{packages/editor/updater-cli,packages/monorepo-tooling/code-insights,packages/monorepo-tooling/dependency-version-analytics,packages/monorepo-tooling/branch-deploy-product-integrator}' -- cp ./tmp/package.json ./dist/version.json",
    "update:icons": "                        bolt w @atlaskit/icon update && bolt w @atlaskit/icon-object update && bolt w @atlaskit/icon-file-type update && bolt w @atlaskit/reduced-ui-pack update",
    "delete": "                              yarn delete:modules && yarn delete:build:artifacts",
    "delete:modules": "                      git clean -dfX --dry-run | grep  node_modules | xargs rm -rf",
    "delete:build:artifacts": "              git clean -dfX --dry-run | grep -v node_modules | xargs rm -rf",
    "enable:lfs": "                          node enable-lfs-hooks.js",
    "test": "                                yarn jest",
    "test:changed": "                        CHANGED_PACKAGES=$(node build/ci-scripts/get.changed.packages.since.master.js) COVERAGE_PACKAGES=$(node build/ci-scripts/get.code.threshold.for.changed.packages.since.master.js) yarn test",
    "test:browser": "                        projector ./projector testBrowser",
    "test:browser:watch": "                  projector ./projector testBrowser --watch",
    "test:browser:browserstack": "           projector ./projector testBrowserCI --browserstack",
    "test:webdriver": "                      node ./build/webdriver-runner/index.js",
    "test:webdriver:watch": "                WATCH=true node ./build/webdriver-runner/index.js",
    "test:webdriver:watch:chrome": "         HEADLESS=false WATCH=true node ./build/webdriver-runner/index.js",
    "test:webdriver:browserstack": "         TEST_ENV=browserstack node ./build/webdriver-runner/index.js",
    "test:webdriver:browserstack:changed": " CHANGED_PACKAGES=$(node build/ci-scripts/get.changed.packages.since.master.js) yarn test:webdriver:browserstack",
    "test:vr": "                             git lfs pull && node ./build/visual-regression/index.js",
    "test:vr:changed": "                     CHANGED_PACKAGES=$(node build/ci-scripts/get.changed.packages.since.master.js) yarn test:vr",
    "test:vr:dependents": "                  CHANGED_PACKAGES=$(node build/ci-scripts/get.changed.packages.since.master.js --dependents='direct') yarn test:vr",
    "test:editor-mobile-bridge:android": "   USER_AGENT='Android 8.0' yarn test --config jest.config.editor-mobile-bridge.js",
    "test:editor-mobile-bridge:ios": "       USER_AGENT='AppleWebKit/605.1.15 Mobile/15E148 Safari/604.1' yarn test --config jest.config.editor-mobile-bridge.js",
    "lint": "                                yarn run lint:eslint && yarn run lint:stylelint",
    "lint:stylelint": "                      stylelint '**/*.{js,ts,tsx}'",
    "lint:eslint": "                         eslint '**/*.{js,ts,tsx}'",
    "lint:checkPrettier": "                  eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "typecheck": "                           yarn run typecheck:flow && yarn run typecheck:typescript",
    "typecheck:flow": "                      flow check --max-warnings=0",
    "typecheck:typescript": "                NODE_OPTIONS=--max-old-space-size=4096 tsc --noEmit --project ./tsconfig.typecheck.json",
    "measure": "                             measure",
    "measure:dependents:s3": "               CHANGED_MAIN_PACKAGES=$(node build/ci-scripts/get.changed.packages.since.master.js) yarn measure:dependents --s3",
    "measure:changed": "                     node build/ci-scripts/get.changed.packages.since.master.js --spaceDelimited --only='packages' --exclude='monorepo-tooling' | xargs -n1 -P2 yarn measure --lint",
    "measure:dependents": "                  node build/ci-scripts/get.changed.packages.since.master.js --dependents='direct' --spaceDelimited --only='packages' --exclude='monorepo-tooling' | xargs -n1 -P2 yarn measure --lint",
    "measure:updateSnapshot": "              node build/ci-scripts/get.changed.packages.since.master.js --spaceDelimited --only='packages' --exclude='monorepo-tooling' | xargs -n1 -P2 yarn measure --updateSnapshot",
    "website:start": "                       cd website && yarn start",
    "send:buildEvents": "                    node build/ci-scripts/sending.build.events.payload.js",
    "send:stepFailureEvents": "              node build/ci-scripts/sending.step.failure.events.payload.js",
    "send:bundleSize": "                     node build/ci-scripts/sending.bundle.size.analytics.js",
    "start": "                               yarn run website:start",
    "start:bitbucket": "                     yarn run website:start **/packages/bitbucket/**",
    "start:core": "                          yarn run website:start **/packages/core/**",
    "start:css-packs": "                     yarn run website:start **/packages/css-packs/**",
    "start:editor": "                        yarn run website:start **/packages/editor/**",
    "start:elements": "                      yarn run website:start **/packages/elements/**",
    "start:engagement-platform": "           yarn run website:start **/packages/engagement-platform/**",
    "start:growth": "                        yarn run website:start **/packages/growth/**",
    "start:home": "                          yarn run website:start **/packages/home/**",
    "start:jira": "                          yarn run website:start **/packages/jira/**",
    "start:media": "                         yarn run website:start **/packages/media/**",
    "start:navigation": "                    yarn run website:start **/packages/navigation/**",
    "start:people-and-teams": "              yarn run website:start **/packages/people-and-teams/**",
    "start:search": "                        yarn run website:start **/packages/search/**",
    "start:trello": "                        yarn run website:start **/packages/trello/**",
    "start:teamwork-ecosystem": "            yarn run website:start **/packages/teamwork-ecosystem/**"
  },
